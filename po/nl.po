#~ msgstr "Wat is dat ding!?!"

#~ msgid "Eloh protect us!"
#~ msgstr "Eloh bescherm ons!"

#~ msgid "Curse Uria, more abominations!"
#~ msgstr "Vervloek Uria, meer gruwelen!"

