#~ msgid "Lt. Illan"
#~ msgstr "Löjtnant Illan"

#~ msgid "Rheban"
#~ msgstr "Rheban"

#~ msgid "Jul"
#~ msgstr "Jul"

#~ msgid "Lt. Meris"
#~ msgstr "Löjtnant Meris"

#~ msgid "Findlas"
#~ msgstr "Findlas"

#~ msgid "Chumet"
#~ msgstr "Chumet"

