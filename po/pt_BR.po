#~ msgstr "Stalrag"

#~ msgid "Gomdrsil"
#~ msgstr "Gomdrsil"

#~ msgid "Duldurus"
#~ msgstr "Duldurus"

#~ msgid "Peldril"
#~ msgstr "Peldril"

